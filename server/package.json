{
  "name": "heimdall.js-server",
  "version": "1.0.0",
  "description": "Heimdall Server",
  "type": "module",
  "main": "/src/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node --loader ts-node/esm.mjs --experimental-top-level-await --experimental-specifier-resolution=node src/server.ts",
    "dev": "cross-env NODE_ENV=development nodemon --watch src --delay 1 --exec 'ts-node' src/server.ts",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --fix --ext ts src/",
    "tsc": "tsc --project tsconfig.json",
    "woow": "node --experimental-specifier-resolution=node ./dist/server.js"
  },
  "author": "Joshua Schnabel",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "cli-color": "^2.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "fast-glob": "^3.2.4",
    "helmet": "^3.23.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^1.4.0",
    "js-yaml": "^3.14.0",
    "lodash-es": "^4.17.15",
    "log4js": "^6.3.0",
    "multer": "^1.4.2",
    "pug": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.4",
    "swagger-ui-express": "^4.1.4",
    "ts-node": "^8.10.2",
    "tsyringe": "^4.3.0",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cli-color": "^2.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/glob": "^7.1.2",
    "@types/helmet": "0.0.47",
    "@types/hpp": "^0.2.1",
    "@types/js-yaml": "^3.12.4",
    "@types/lodash": "^4.14.155",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.14",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unused-imports": "^0.1.3",
    "nodemon": "^2.0.4"
  }
}
